{
  "name": "n8n-nodes-dante-pdf",
  "version": "2.0.0",
  "description": "n8n custom node for PDF conversion with support for Markdown, Text, DOCX, HTML, Image to PDF and PDF merging",
  "keywords": [
    "n8n-custom-node",
    "pdf",
    "conversion",
    "markdown",
    "html",
    "docx",
    "image",
    "merge"
  ],
  "license": "MIT",
  "homepage": "https://github.com/dandacompany/n8n-nodes-dante-pdf",
  "author": {
    "name": "Dante PDF",
    "email": "datapod.k@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dandacompany/n8n-nodes-dante-pdf.git"
  },
  "bugs": {
    "url": "https://github.com/dandacompany/n8n-nodes-dante-pdf/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "n8n": {
    "nodes": [
      "dist/nodes/DantePdf/DantePdf.node.js"
    ]
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.json && npm run copy:assets",
    "copy:assets": "cp -r src/nodes/DantePdf/*.png dist/nodes/DantePdf/ 2>/dev/null || true",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,md,json}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,md,json}'",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "tsc -p tsconfig.json --watch",
    "link": "npm run build && node -e \"require('fs').cpSync('./dist', process.env.HOME+'/.n8n/custom/n8n-nodes-dante-pdf', {recursive:true, force:true}) && require('fs').cpSync('./package.json', process.env.HOME+'/.n8n/custom/n8n-nodes-dante-pdf/package.json', {force:true})\"",
    "prepack": "tsc -p tsconfig.json && npm run copy:assets",
    "postinstall": "node scripts/setup-dependencies.js"
  },
  "files": [
    "dist/**/*",
    "scripts/**/*",
    "package.json",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node-fetch": "^2.6.4",
    "detect-libc": "^2.1.0",
    "fs-extra": "^11.3.1",
    "jsdom": "^24.0.0",
    "mammoth": "^1.7.0",
    "marked": "^12.0.0",
    "node-fetch": "^2.6.7",
    "node-html-parser": "^6.1.0",
    "pdf-lib": "^1.17.0",
    "pdfkit": "^0.15.0",
    "playwright-core": "^1.55.0",
    "sharp": "^0.33.0",
    "tsconfig-paths": "^4.2.0",
    "wkhtmltopdf": "^0.4.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/jsdom": "^21.0.0",
    "@types/node": "^20.0.0",
    "@types/pdfkit": "^0.13.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.0.0",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.0.0"
  }
}
